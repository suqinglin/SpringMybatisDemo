<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.suql.mapper.UserMapper">
    <insert id="insert" parameterType="com.suql.bean.UserInfo">
        insert into suql_user(user_name, user_age, user_sex)
        values(#{userName}, #{age}, #{sex})
    </insert>
</mapper>
